    "test:e2e:trim-manual": "npx ts-node tests/selenium/trim-manual.test.ts",
    "test:e2e:repeated-trim": "npx ts-node tests/selenium/repeated-trim.test.ts",
    "test:e2e:draw-polyline": "npx ts-node tests/selenium/draw-polyline.test.ts",
    "test:e2e:draw-arc": "npx ts-node tests/selenium/draw-arc.test.ts",
